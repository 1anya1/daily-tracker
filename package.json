{
  "name": "tracker-app",
  "version": "1.0.0",
  "description": "Track your daily todos with this app.",
  "main": "server.js",
  "scripts": {
    "build-css": "node-sass public/scss-stylesheets -o public/stylesheets",
    "watch-css": "npm run build-css && node-sass public/scss-stylesheets -o public/stylesheets --watch --recursive",
    "start-js": "node server.js",
    "start": "npm-run-all -p watch-css start-js",
    "test": "react-scripts test --env=jsdom"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "mongoose": "^5.10.5",
    "node-sass": "^4.14.1"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1anya1/daily-tracker.git"
  },
  "bugs": {
    "url": "https://github.com/1anya1/daily-tracker/issues"
  },
  "homepage": "https://github.com/1anya1/daily-tracker#readme"
}
